{
  "name": "MustStash",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Enhanced personalized search tool",
  "icons": {
    "16": "muststash_temp_icon1.png",
    "48": "muststash_temp_icon1.png",
    "128": "muststash_temp_icon1.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "MustStash",
    "default_icon": "muststash_temp_icon1.png"
  },
  "options_page": "options.html",
  "permissions": [
  "background",
  "tabs",
  "http://*/*",
  "https://*/*"
  ]
}